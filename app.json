{
  "expo": {
    "name": "VocaMan",
    "slug": "vocaman",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F7FA"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png",
      "staticDirectory": "./web-build"
    },
    "android": {
      "package": "com.yourcompany.vocaman",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#F5F7FA"
      },
      "permissions": []
    }
  }
}
